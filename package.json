{
  "name": "ezpp!",
  "version": "1.3.3",
  "description": "Browser extension for calculating pp!",
  "main": "src/js/index.js",
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.1",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.9.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.28.5",
    "extract-text-webpack-plugin": "^3.0.0",
    "node-sass": "^3.7.0",
    "osu-parser-web": "git+https://github.com/oamaok/osu-parser-web.git",
    "osu-pp-calculator": "^0.1.6",
    "sass-loader": "^6.0.6",
    "showdown": "^1.7.2",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.0",
    "eslint": "^3.0.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2"
  },
  "scripts": {
    "start:chrome": "BUILD_CHROME=1 NODE_ENV=development webpack --config webpack.development.config.js -w",
    "start:firefox": "BUILD_FF=1 NODE_ENV=development webpack --config webpack.development.config.js -w",
    "build:chrome": "rm -rf dist/ ezpp.zip ezpp.xpi && BUILD_CHROME=1 NODE_ENV=production webpack --config webpack.production.config.js && zip -jr ezpp.zip dist/*",
    "build:firefox": "rm -rf dist/ ezpp.zip ezpp.xpi && BUILD_FF=1 NODE_ENV=production webpack --config webpack.production.config.js && zip -jr ezpp.xpi dist/*",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oamaok/ezpp.git"
  },
  "author": "Teemu Pääkkönen <oamaok@gmail.com>",
  "license": "SEE LICENSE IN ./LICENSE",
  "bugs": {
    "url": "https://github.com/oamaok/ezpp/issues"
  },
  "homepage": "https://github.com/oamaok/ezpp#readme"
}
